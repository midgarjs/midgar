{
  "name": "@midgar/midgar",
  "version": "1.0.0-alpha.2.2",
  "description": "A node js framework over express",
  "main": "./src/index.js",
  "scripts": {
    "build-docs": "jsdoc -c ./jsdoc.js ./README.md -t ./node_modules/ink-docstrap/template ./README.md -r ./src/",
    "test": "mocha --require esm tests/*.js --reporter mocha-multi-reporters --reporter-options configFile=mocha-multi-reporters.json --exit",
    "eslint": "eslint -f json -o reports/.eslint-report.json src/ || exit 0",
    "coverage": "nyc -i esm --reporter=lcov npm run test"
  },
  "module": "true",
  "bin": {
    "midgar": "./src/bin/midgar.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/midgarjs/midgar.git"
  },
  "esm": {
    "cjs": true,
    "sourceMap": true
  },
  "dependencies": {
    "@midgar/utils": "^1.0.0-alpha.1.5",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "commander": "^4.0.1",
    "emittery": "^0.5.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "figlet": "^1.2.4",
    "helmet": "^3.21.2",
    "mkdirp": "^0.5.1",
    "mocha-junit-reporter": "^1.23.1",
    "rimraf": "^3.0.0",
    "save": "^2.4.0",
    "uid-safe": "^2.1.5",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "sinon": "^8.0.2",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "chai-fs": "^2.0.0",
    "chai-http": "^4.3.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "3.6.3",
    "mocha": "^6.2.2",
    "mocha-multi-reporters": "^1.1.7",
    "mocha-sonarqube-reporter": "^1.0.1",
    "nyc": "^15.0.0"
  },
  "author": "Metais Fabien",
  "license": "MIT"
}
